<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules xmlns:tools="http://schemas.android.com/tools">
    <cloud-backup>
        <include tools:ignore="FullBackupContent">
            <!-- Specify what data to include in cloud backup here -->
            <file path="/path/to/important/file.txt" />
            <contacts />
            <!-- Add more data to include as needed -->
        </include>
        <exclude>
            <!-- Specify what data to exclude from cloud backup here -->
            <cache />
            <logs />
            <!-- Add more data to exclude as needed -->
        </exclude>
    </cloud-backup>
    <device-transfer>
        <include tools:ignore="FullBackupContent">
            <!-- Specify what data to include in device transfer here -->
            <photos />
            <calendar />
            <!-- Add more data to include as needed -->
        </include>
        <exclude>
            <!-- Specify what data to exclude from device transfer here -->
            <system-apps />
            <!-- Add more data to exclude as needed -->
        </exclude>
    </device-transfer>
</data-extraction-rules>
